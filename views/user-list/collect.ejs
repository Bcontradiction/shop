<div class="container views-userlist-collect">
    <!--导航-->
    <div class="user-collect vi-co-title">
        <ul class="nav aw-nav-tabs active">
            <h4>
                <i class="fa fa-heart" aria-hidden="true"></i>&nbsp;收藏的列表
            </h4>
        </ul>
    </div>
    <ul class="vi-co-list">
        <% users.collect.forEach(function(users){%>
        <li class="v-c-l-li">
            <div class="v-c-l-l-img flort-left">
                <a href="">
                    <img src="<%=users.pictures%>" alt="">
                </a>
            </div>
            <div class="v-c-l-l-info flort-left">
                <p><%=users.shopName%></p>
                <p>$<%=users.price%></p>
            </div>
            <div class="v-c-l-l-author flort-left">
                <span>发布人</span>
                <a href=""><span><%= users.userName%></span></a>
            </div>
            <div class="v-c-l-l-time flort-left">
                <span>发布时间</span>
                <span><%=users.create_time%></span>
            </div>
            <div class="click-shop-buy v-c-l-l-btn flort-right" target="">
                <span><a href="/shop/<%=users.shop_id %>">进入商品编辑页面</a></span>
            </div>
        </li>
        <%})%>

    </ul>
</div>
